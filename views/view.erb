<header>
<a href="/"><h1>Hem</h1></a>
</header>
<div class="content">
	<%posts.each do |i|%>
		<div class="box">
			<div class="box1">
				<p class="title"><%= i[4]%></p>
				<p class="desc"><%= i[5]%></p>
			</div>
			<p class="date"><%= i[1]%></p>
			<div class="box2">
				<p class="likes"><%= i[2]%></p>
				<p class="dlikes"><%= i[3]%></p>
			</div>
			</div>
	<%end%>
</div>